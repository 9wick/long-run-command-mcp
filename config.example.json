{
  "outputdir": "./logs",
  "commands": {
    "build": {
      "workdir": "./",
      "command": "npm run build"
    },
    "test": {
      "workdir": "./",
      "command": "npm test"
    },
    "lint": {
      "workdir": "./",
      "command": "npm run lint"
    },
    "typecheck": {
      "workdir": "./",
      "command": "npm run check:type"
    }
  }
}
